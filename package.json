{
  "name": "strap",
  "version": "0.0.1",
  "description": "Install packages with ease.",
  "author": "Morten Scheel @mortenscheel",
  "bin": {
    "strap": "./bin/run"
  },
  "homepage": "https://github.com/mortenscheel/strap",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "mortenscheel/strap",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@types/execa": "^2.0.0",
    "@types/inquirer": "^8.2.6",
    "@types/js-yaml": "^4.0.5",
    "@types/listr": "^0.14.4",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.0",
    "execa": "^5.1.1",
    "inquirer": "^8.2.6",
    "js-yaml": "^4.1.0",
    "listr": "^0.14.3",
    "open": "^9.1.0",
    "slugify": "^1.6.6",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.4.4",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.40",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3.11.3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "strap",
    "dirname": "strap",
    "commands": "./dist/commands",
    "default": "install",
    "plugins": [],
    "macos": {
      "identifier": "com.mortenscheel.strap"
    },
    "topicSeparator": ":",
    "topics": {}
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lint-fix": "npm run lint -- --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/mortenscheel/strap/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
